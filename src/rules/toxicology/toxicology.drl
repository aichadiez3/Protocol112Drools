package rules.toxicology;

//list any import classes here.
import pojos.Emergency;
import jdbc.SQLManager;


rule "food_poisoning"

	dialect "java"

	when
		dbm : SQLManager(  )
		u : Emergency (getDisease().getName().equals("Food poisoning"))
	then

		u.setSeverity(3);
		u.setProtocol(dbm.Search_protocol_by_id(7));
end
